    "axios": "^1.5.1",
    "cheerio": "^1.0.0-rc.12",
    "ws": "^8.14.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "rate-limiter-flexible": "^2.4.2",
    "user-agents": "^1.0.1384",
    "zod": "^3.22.4"
